            _id: "6392339d8579307434af8797",
            search: '"javaScript" "Top K Frequent Elements"',
            credit:
              "http://maxinrui.com/index.php/2018/06/21/top-k-frequent-elements/",
            identifier: "question11255029",
            answer:
              "This answer, plus a write-up, can be found at:\nhttp://maxinrui.com/index.php/2018/06/21/top-k-frequent-elements/\n\n\t\n/**\n * @param {number[]} nums\n * @param {number} k\n * @return {number[]}\n */\nvar topKFrequent = function(nums, k) {\n    let map = {};\n    let sortable = [];\n    let result = [];\n    for (let number of nums) {\n        map[number] = map[number] ? ++map[number] : 1;\n    }\n    for (let key in map) {\n        sortable.push([key, map[key]]);\n    }\n    sortable.sort(function(a, b) {\n        return b[1] - a[1];\n    })\n    sortable = sortable.slice(0, k);\n    for (let key of sortable) {\n       result.push(parseInt(key));\n    }\n    return result;\n};",
