        question116317119: {
          _id: "6392339d8579307434af87a8",
          topic: "algorithms",
          search: '"JavaScript" "Longest Consecutive Sequence"',
          title: "Longest Consecutive Sequence",
          link: "https://www.cnblogs.com/liok3187/p/4542262.html",
          answer:
            "This answer comes from:\nhttps://www.cnblogs.com/liok3187/p/4542262.html\n\n\n/**\n * @param {number[]} nums\n * @return {number}\n */\nvar longestConsecutive = function(nums) {\n    var set = new Set();\n    for(var i = 0; i < nums.length; i++){\n        if(!set.has(nums[i])){\n            set.add(nums[i]);\n        }\n    }\n    var max = 0;\n    var count = 0;\n    for(i in nums){\n        curr = nums[i];\n        if(set.has(nums[i])){\n            count = 1;\n            set.delete(nums[i]);\n        }\n        while(set.has(curr - 1)){\n            if(set.has(curr - 1)){\n                count ++;\n                set.delete(curr - 1);\n                curr -= 1;\n            }\n        }\n        curr = nums[i];\n        while(set.has(curr + 1)){\n            if(set.has(curr + 1)){\n                count ++;\n                set.delete(curr + 1);\n                curr += 1;\n            }\n        }\n\n        if(count > max){\n            max = count;\n        }\n    }\n\n    return max;\n};",
