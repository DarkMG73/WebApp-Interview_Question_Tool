              "https://leetcode.com/problems/move-zeroes/discuss/72422/JavaScript-solution\n\nfunction moveZeroes(nums) {\n  var idx = 0;\n  for (var i = 0; i < nums.length; i++) {\n    if (nums[i] !== 0) {\n      nums[idx] = nums[i];\n      nums[i] = idx === i ? nums[i] : 0;\n      idx++;\n    }\n  }\n}",
